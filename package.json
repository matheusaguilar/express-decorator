{
  "name": "@maguine/express-decorator",
  "version": "1.0.0",
  "description": "Typescript Decorators to use with Express Routing",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "clean": "gulp",
    "build": "npm run clean && tsc",
    "lint": "npx eslint **/*.{ts} --quiet --no-error-on-unmatched-pattern --fix",
    "commit": "git-cz",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": ["Typescript", "typescript", "Express", "express", "Decorator", "decorator"],
  "author": "Matheus Catarino de Aguilar <matcatarino@yahoo.com.br>",
  "license": "MIT",
  "private": false,
  "dependencies": {},
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "commitizen": "^4.0.4",
    "cz-conventional-changelog": "^3.1.0",
    "del": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "express": "^4.17.1",
    "gulp": "^4.0.2",
    "husky": "^4.2.5",
    "prettier": "^2.0.4",
    "reflect-metadata": "^0.1.13",
    "typescript": "^3.8.3",
    "validate-commit-msg": "^2.14.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "build",
        "ci",
        "chore",
        "revert"
      ]
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "validate-commit-msg"
    }
  }
}
